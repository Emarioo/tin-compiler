struct Vector1 { x: float }
struct Vector2 { x: float, y: float }
struct Vector3 { x: float, y: float, z: float }
struct Vector4 { x: float, y: float, z: float, w: float }
struct Quaternion { x: float, y: float, z: float, w: float }
struct Matrix4 { v0: Vector4, v1: Vector4, v2: Vector4, v3: Vector4 }